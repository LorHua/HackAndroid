<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="8dip"/>
            <solid android:color="@color/orange_press"/>
            <stroke android:width="0.5dip" android:color="@color/gray" android:dashGap="0dip" android:dashWidth="1dip"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="8dip"/>
            <solid android:color="@color/orange_press"/>
            <stroke android:width="0.5dip" android:color="@color/gray" android:dashGap="0dip" android:dashWidth="1dip"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="8dip"/>
            <solid android:color="@color/orange_press"/>
            <stroke android:width="0.5dip" android:color="@color/gray" android:dashGap="0dip" android:dashWidth="1dip"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="8dip"/>
            <solid android:color="@color/cashbox300_gray"/>
            <stroke android:width="0.5dip" android:color="@color/gray" android:dashGap="0dip" android:dashWidth="1dip"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="8dip"/>
            <solid android:color="@color/orange_nor"/>
            <stroke android:width="0.5dip" android:color="@color/gray" android:dashGap="0dip" android:dashWidth="1dip"/>
        </shape>
    </item>

</selector>
